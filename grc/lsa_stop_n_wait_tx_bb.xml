<?xml version="1.0"?>
<block>
  <name>SNS SU TX</name>
  <key>lsa_stop_n_wait_tx_bb</key>
  <category>[lsa]</category>
  <import>import lsa</import>
  <make>lsa.stop_n_wait_tx_bb($tagname,$filename,$usef,$verb,$send,$mean,$std)</make>
  <callback>set_send($send)</callback>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Tagname</name>
    <key>tagname</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <param>
    <name>File</name>
    <key>filename</key>
    <value></value>
    <type>file_open</type>
  </param>
  <param>
    <name>Use file</name>
    <key>usef</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
  </param>
  <param>
    <name>Verbose</name>
    <key>verb</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
  </param>
  <param>
    <name>Send size</name>
    <key>send</key>
    <value>10</value>
    <type>int</type>
  </param>
  <param>
    <name>Mean</name>
    <key>mean</key>
    <value>1000</value>
    <type>float</type>
  </param>
  <param>
    <name>Std</name>
    <key>std</key>
    <value>100</value>
    <type>float</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <sink>
    <name>msg_in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
