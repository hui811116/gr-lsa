<?xml version="1.0"?>
<block>
  <name>Interference Cancellation Core</name>
  <key>lsa_interference_cancellation_core_cc</key>
  <category>[lsa]</category>
  <import>import lsa</import>
  <make>lsa.interference_cancellation_core_cc($debug)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>On</name>
      <key>True</key>
    </option>
    <option>
      <name>Off</name>
      <key>False</key>
    </option>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <sink>
    <name>phase</name>
    <type>float</type>
  </sink>
  <sink>
    <name>time</name>
    <type>float</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
