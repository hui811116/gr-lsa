<?xml version="1.0"?>
<block>
  <name>Control Source</name>
  <key>lsa_control_source_b</key>
  <category>[lsa]</category>
  <import>import lsa</import>
  <make>lsa.control_source_b($seed)</make>
  
  <param>
    <name>Seed</name>
    <key>seed</key>
    <type>int</type>
  </param>

  
  <sink>
    <name>pld</name>
    <type>message</type>
  </sink>

  
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
