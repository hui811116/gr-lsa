<?xml version="1.0"?>
<block>
  <name>Expand Sync</name>
  <key>lsa_expand_symbal_to_sample_ff</key>
  <category>[lsa]</category>
  <import>import lsa</import>
  <make>lsa.expand_symbal_to_sample_ff($sps)</make>
  <!--<make>lsa.expand_symbal_to_sample_ff($sps, $nfilts)</make> -->
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Samples per symbol</name>
    <key>sps</key>
    <type>int</type>
  </param>
  <!--
  <param>
    <name>Number of filters</name>
    <key>nfilts</key>
    <type>int</type>
  </param>
  -->


  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>phase</name>
    <type>float</type>
  </sink>
  <sink>
    <name>freq</name>
    <type>float</type>
  </sink>
  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out phase</name>
    <type>float</type>
  </source>
  <source>
    <name>out freq</name>
    <type>float</type>
  </source>
</block>
