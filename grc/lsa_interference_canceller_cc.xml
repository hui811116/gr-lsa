<?xml version="1.0"?>
<block>
  <name>LSA Interference Canceller</name>
  <key>lsa_interference_canceller_cc</key>
  <category>[lsa]</category>
  <import>import lsa</import>
  <make>lsa.interference_canceller_cc($preamble, $sense,$sps, $bps, $hdr_bits,$debug)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>clean preamble</name>
    <key>preamble</key>
    <type>complex_vector</type>
  </param>
  <param>
    <name>sensing tagname</name>
    <key>sense</key>
    <value>"sensing"</value>
    <type>string</type>
  </param>
  <param>
    <name>Debug mode</name>
    <key>debug</key>
    <value>False</value>
    <type>string</type>
    <option>
      <name>On</name>
      <key>True</key>
    </option>
    <option>
      <name>Off</name>
      <key>False</key>
    </option>
  </param>
  <param>
    <name>Sample/symbol</name>
    <key>sps</key>
    <type>int</type>
  </param>
  <param>
    <name>Bits per symbol</name>
    <key>bps</key>
    <type>int</type>
  </param>
  <param>
    <name>header bits number</name>
    <key>hdr_bits</key>
    <type>int</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  <source>
    <name>energy</name>
    <type>float</type>
    <optional>1</optional>
  </source>
</block>
