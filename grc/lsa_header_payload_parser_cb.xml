<?xml version="1.0"?>
<block>
  <name>Header Payload Parser</name>
  <key>lsa_header_payload_parser_cb</key>
  <category>[lsa]</category>
  <import>import lsa</import>  
  <import>from gnuradio import digital</import>
  <make>lsa.header_payload_parser_cb($hdr_constellation,$pld_constellation,$symbols,$accesscode,$threshold)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Header Constellation</name>
    <key>hdr_constellation</key>
    <type>raw</type>
  </param>
  <param>
    <name>Payload Constellation</name>
    <key>pld_constellation</key>
    <type>raw</type>
  </param>
  <param>
    <name>Preamble Symbols</name>
    <key>symbols</key>
    <type>complex_vector</type>
  </param>
  <param>
    <name>Accesssbits(ub)</name>
    <key>accesscode</key>
    <type>string</type>
  </param>
    <param>
    <name>Correlation Threshold</name>
    <key>threshold</key>
    <value>0.5</value>
    <type>float</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  <source>
    <name>payload</name>
    <type>message</type>
    <optional>1</optional>
  </source>
  <source>
    <name>header bytes</name>
    <type>byte</type>
    <optional>1</optional>
  </source>
</block>
