<?xml version="1.0"?>
<block>
  <name>Symbol level Canceller</name>
  <key>lsa_symbol_level_ic_cc</key>
  <category>[lsa]</category>
  <import>import lsa</import>
  <make>lsa.symbol_level_ic_cc($access, $clean, $bps, $debug)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Accesscode</name>
    <key>access</key>
    <type>string</type>
  </param>
  <param>
    <name>Clean Preamble</name>
    <key>clean</key>
    <type>complex_vector</type>
  </param>
  <param>
    <name>Bits per symbol(hdr)</name>
    <key>bps</key>
    <type>int</type>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>On</name>
      <key>True</key>
    </option>
    <option>
      <name>Off</name>
      <key>False</key>
    </option>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <sink>
    <name>phase</name>
    <type>float</type>
    <optional>1</optional>
  </sink>
  <sink>
    <name>rate_k</name>
    <type>float</type>
    <optional>1</optional>
  </sink>
  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  <source>
    <name>energy</name>
    <type>float</type>
    <optional>1</optional>
  </source>
</block>
