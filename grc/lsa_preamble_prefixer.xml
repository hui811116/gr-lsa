<?xml version="1.0"?>
<block>
  <name>Preamble Prefixer</name>
  <key>lsa_preamble_prefixer</key>
  <category>[lsa]</category>
  <import>import lsa</import>
  <make>lsa.preamble_prefixer($user)</make>

  <param>
    <name>User</name>
    <key>user</key>
    <value>0</value>
    <type>int</type>
    <option>
      <name>ProU</name>
      <key>0</key>
    </option>
    <option>
      <name>SU</name>
      <key>1</key>
    </option>
  </param>


  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
