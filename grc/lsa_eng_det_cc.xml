<?xml version="1.0"?>
<block>
  <name>Energy Detector</name>
  <key>lsa_eng_det_cc</key>
  <category>[lsa]</category>
  <import>import lsa</import>
  <make>lsa.eng_det_cc($threshold)</make>
  <callback>set_threshold($threshold)</callback>
  <param>
    <name>Threshold(dB)</name>
    <key>threshold</key>
    <type>float</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <sink>
    <name>ed</name>
    <type>float</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  
</block>
